<% content_for :drawer do %>
  <%= render Maglev::Uikit::PageLayoutComponent.new(back_path: editor_real_root_path(maglev_editing_route_context)) do |page_layout| %>
    <% page_layout.with_title_content t('maglev.editor.sections.list_pane.title') %>
    <% page_layout.with_description_content t('maglev.editor.sections.list_pane.sub_title') %>
    <% page_layout.with_notification_content maglev_flash_message %>

    <%= render 'list' %>
  <% end %>
<% end %>