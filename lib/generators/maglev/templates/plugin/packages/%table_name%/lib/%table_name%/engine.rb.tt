# frozen_string_literal: true

module <%= class_name %>
  class Engine < ::Rails::Engine
    isolate_namespace <%= class_name %>

    initializer 'maglev.plugins.<%= table_name %>' do
      Maglev.register_plugin(
        id: '<%= name %>',
        name: '<%= human_name %>',
        version: <%= class_name %>::VERSION,
        root_path: root
      )      
    end
  end
end